<?xml version="1.0" encoding="utf-8"?>
    <FrameLayout android:id="@+id/frameLayout1" android:layout_width="fill_parent" android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android">
        <ScrollView android:layout_height="match_parent" android:layout_width="match_parent" android:id="@+id/scrollView1" android:descendantFocusability="blocksDescendants">
            <TableLayout android:id="@+id/tableLayout1" android:layout_height="match_parent" android:descendantFocusability="beforeDescendants" android:focusableInTouchMode="true" android:layout_width="match_parent" android:focusable="true">
                <TableRow android:id="@+id/tableRow1" android:layout_width="wrap_content" android:layout_height="wrap_content">
                    <TextView android:id="@+id/textView5" android:text="Hero" android:layout_width="wrap_content" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_height="wrap_content"></TextView>
                </TableRow>
                <TableRow android:id="@+id/tableRow3" android:layout_width="wrap_content" android:layout_height="wrap_content">
                    <TextView android:id="@+id/textViewImage" android:text="@string/actorImage" android:layout_height="wrap_content" android:layout_width="150dip"></TextView>
                    <edu.elon.honors.price.maker.SelectorActorImage android:id="@+id/spinnerActorImage" android:layout_height="wrap_content" android:layout_width="wrap_content" android:minWidth="200dip"></edu.elon.honors.price.maker.SelectorActorImage>
                </TableRow>
                <TableRow android:id="@+id/tableRow5" android:layout_width="wrap_content" android:layout_height="wrap_content">
                    <TextView android:id="@+id/textView3" android:text="@string/speed" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
                    <SeekBar android:layout_width="150dip" android:id="@+id/seekBarSpeed" android:layout_height="wrap_content"></SeekBar>
                </TableRow>
                <TableRow android:id="@+id/tableRow6" android:layout_width="wrap_content" android:layout_height="wrap_content">
                    <TextView android:id="@+id/textView4" android:text="Jump Height:" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
                    <SeekBar android:layout_width="150dip" android:id="@+id/seekBarJump" android:layout_height="wrap_content"></SeekBar>
                </TableRow>
                <TableRow android:id="@+id/tableRow7" android:layout_width="wrap_content" android:layout_height="wrap_content">
                    <TextView android:id="@+id/textView6" android:text="Event:" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
                    <Spinner android:layout_width="wrap_content" android:id="@+id/spinnerEvent" android:focusable="true" android:layout_height="wrap_content" android:focusableInTouchMode="true"></Spinner>
                </TableRow>
                <TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content" android:layout_height="wrap_content">
                    <TextView android:id="@+id/TextView01" android:text="Direction:" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
                    <Spinner android:visibility="invisible" android:layout_width="wrap_content" android:id="@+id/spinnerDirection" android:layout_height="wrap_content"></Spinner>
                </TableRow>
                <TableRow android:id="@+id/tableRow8" android:layout_width="wrap_content" android:layout_height="wrap_content">
                    <TextView android:id="@+id/textView7" android:text="Behavior:" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
                    <Spinner android:layout_width="wrap_content" android:id="@+id/spinnerBehavior" android:layout_height="wrap_content"></Spinner>
                </TableRow>
                <TableRow android:id="@+id/tableRow4" android:layout_width="wrap_content" android:layout_height="wrap_content"></TableRow>
            </TableLayout>
        </ScrollView>
    </FrameLayout>
